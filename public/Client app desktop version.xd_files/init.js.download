(function(){document.addEventListener("DOMContentLoaded",function(){const btn=document.querySelector("#fixLocalClient");if(btn){btn.addEventListener("click",function(){localStorage.removeItem("_installedVersion");caches.keys().then(function(keys){Promise.all(keys.map(function(key){return caches.delete(key);})).then(function(){window.location.reload();})});});}});try{if(navigator.serviceWorker){navigator.serviceWorker.register("/sw.js").then((reg)=>{reg.update();})}}catch(e){}})();